<div class="user-login">
  <h1>Log In</h1>
  <div class="user-login-form">
    <%= form_tag session_create_path do %>
    <%= label_tag :username %>
    <%= text_field_tag :username %><br/>

    <%= label_tag :password %>
    <%= password_field_tag :password %><br/>

    <%= submit_tag "Log In"%>
    <% end %>
  </div>
</div>